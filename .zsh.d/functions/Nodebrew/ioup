  fast-cli \
  firebase-status \
  fixpack \
  fkill-cli \
  flow-bin \
  gtop \
  ngrok \
  np \
  npm-check \
  planeshift \
  prettier \
  serverless \
  textlint \
  trash-cli \
  70-10/m;
