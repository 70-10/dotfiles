npm cache verify
npm i -g \
  clipboard-cli \
  emoji-cli \
  empty-trash-cli \
  eslint \
  fast-cli \
  firebase-status \
  fixpack \
  fkill-cli \
  flow-bin \
  ngrok \
  np \
  npm-check \
  planeshift \
  prettier \
  serverless \
  textlint \
  trash-cli \
  vtop \
  70-10/world-time;
